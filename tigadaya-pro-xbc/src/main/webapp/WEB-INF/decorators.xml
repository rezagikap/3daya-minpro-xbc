<?xml version="1.0" encoding="ISO-8859-1"?>

<decorators defaultdir="/WEB-INF/decorators">
    <!-- Any urls that are excluded will never be decorated by Sitemesh -->
	<excludes>
		<pattern>/resources/*</pattern>
		<pattern>/trainer/*</pattern>
		<pattern>/technology/*</pattern>
		<pattern>/user/*</pattern>
		<pattern>/role/*</pattern>
		<pattern>/menu/*</pattern>
		<pattern>/menu-access/*</pattern>
		<pattern>/test/*</pattern>
		<pattern>/list/*</pattern>
		<pattern>/biodata/*</pattern>
		<pattern>/category/*</pattern>
		<pattern>/idle-news/*</pattern>
		<pattern>/testimony/*</pattern>
		<pattern>/bootcamptesttype/*</pattern>
		<pattern>/class/*</pattern>
		<pattern>/office/*</pattern>
		<pattern>/batch/*</pattern>
		<pattern>/question/*</pattern>
		<pattern>/logout/*</pattern>
		</excludes>

	<decorator name="main" page="main.jsp">
		<pattern>/*</pattern>
	</decorator>

	<decorator name="home" page="home.jsp">
		<pattern>/home.html</pattern>
	</decorator> 

	<decorator name="login" page="login.jsp">
		<pattern>/login.jsp</pattern>
		<pattern>/login.html</pattern>
	</decorator>
	
</decorators>